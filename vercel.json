{
  "version": 2,
  "builds": [
    {
      "src": "cul_backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cul_backend/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "cul_backend.settings"
  },
  "outputDirectory": "staticfiles_build"
}
